<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" 
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  metal:use-macro="context/@@groupserver_layout/page">
  <head>
    <title metal:fill-slot="title" >
      <span tal:replace="view/title"/>:
      <span tal:replace="view/siteInfo/name"/>
    </title>
  </head>
  <body>
    <h1 metal:fill-slot="pagetitle"
      tal:content="view/title">Page o' Stuff</h1>
    <tal:block metal:fill-slot="body">
      <div id="pageHierarchy">
      </div><!--pageHierarchy-->
      <div id="actions">
        <ul id="primaryActions">
          <li id="add">
            <a title="Add a page beneath the selected page"
              href="#">Add</a>
          </li>
          <li id="copy">
            <a title="Copy the selected page"
              href="#">Copy</a>
          </li>
          <li id="rename">
            <a title="Rename the selected page"
              href="#">Rename</a>
          </li>
          <li id="move">
            <a title="Move the selected page"
              href="#">Move</a>
          </li>
        </ul><!--primaryActions-->
        <ul id="secondaryActions">
          <li>
            <a title="View the selected page" 
              href="#">View&#8230;</a>
          </li>
        </ul><!--secondaryActions-->
      </div><!--actions-->
      <div id="options">
      </div><!--options-->
    </tal:block>
  </body>
</html>

